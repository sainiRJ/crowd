<% if flash.any? %>
<% flash.each do |type, msg| %>
  <div class="alert alert-<%= type %> text-center">
    <%= msg %>
  </div>
<% end %>
<% end %>

<%= form_for [@user, @profile], url: profile_path, method: :post do |f| %>
 
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>  
<%= f.label :profile_photo %>
<%= f.file_field :file %>

<%= f.label :cover_photo %>
<%= f.file_field :video_file %>

  <%= f.submit 'Submit Profile' %>
<% end %>
